(<= (succ ?i3 ?i2 ?i1 ?i0 ?i3 ?j2 ?j1 ?j0)
    (num ?i3 ?i2 ?i1 ?i0)
    (num ?i3 ?j2 ?j1 ?j0)
    (succ ?i2 ?j2)
    (succ ?i1 nan)
    (succ nan ?j1)
    (succ ?i0 nan)
