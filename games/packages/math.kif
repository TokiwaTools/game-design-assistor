(<= (succ ?i3 ?i2 ?i1 ?i0 ?i3 ?i2 ?i1 ?j0)
    (num ?i3 ?i2 ?i1 ?i0)
    (num ?i3 ?i2 ?i1 ?j0)
    (succ ?i0 ?j0))
(<= (succ ?i3 ?i2 ?i1 ?i0 ?i3 ?i2 ?j1 ?j0)
    (num ?i3 ?i2 ?i1 ?i0)
    (num ?i3 ?i2 ?j1 ?j0)
    (succ ?i1 ?j1)
    (succ ?i0 nan)
